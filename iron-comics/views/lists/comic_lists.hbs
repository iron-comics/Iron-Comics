<a href="/lists/create" class="btn" role="button">Create new list</a>
{{#if list}}
    <ul>        
        {{#each list}} 
            <li><a href="lists/list?id={{this.id}}">{{this.name}}</a></li>
            <ul>
            {{#each this.id_comic}}
                <li>{{this.title}}</li>
                <img src="{{this.img_icon}}" alt="">
            {{else}}
                <p>Empty list</p>
            {{/each}}
            </ul>
        {{/each}}
    </ul>
{{else}}
    <h2>No list created</h2>
{{/if}}